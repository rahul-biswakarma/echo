# Gemini API Configuration (Required)
GEMINI_API_KEY=your_gemini_api_key_here

# Qdrant Configuration
# For local Qdrant (default):
QDRANT_URL=http://localhost:6333

# For Qdrant Cloud:
# QDRANT_URL=https://your-cluster-id.region.gcp.cloud.qdrant.io:6334
# QDRANT_API_KEY=your_qdrant_cloud_api_key_here

# Legacy configuration (still supported):
# QDRANT_HOST=localhost
# QDRANT_PORT=6333

# Local Vector Storage Configuration
# No additional configuration needed - vectors are stored in local JSON files
# Default output file: vectors.json
